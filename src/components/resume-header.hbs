{{#resume.basics}}
  <header class="resume-header clearfix">
    <div class="profile-pic">
      {{#if image}}
        <img src="{{image}}" class="rounded-full" alt="profile-pic" />
      {{/if}}
    </div>
    <div class="profile-header">
      <h1>{{name}}</h1>
      <h2>{{label}}</h2>
    </div>
  </header>
{{/resume.basics}}